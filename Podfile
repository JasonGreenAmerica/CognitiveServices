# Uncomment the next line to define a global platform for your project
 platform :ios, '10.0'

target 'cogface' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'ProjectOxfordFace'
  pod 'SwiftyJSON' # https://github.com/SwiftyJSON/SwiftyJSON
  pod 'Alamofire', '~> 4.0' # https://github.com/Alamofire/Alamofire
  pod 'ImageLoader' #https://github.com/hirohisa/ImageLoaderSwift/tree/master/Example/ImageLoaderExample
end
